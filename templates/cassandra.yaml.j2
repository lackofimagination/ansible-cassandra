max_value_size_in_mb: {{ cassandra_max_value_size_in_mb }}
back_pressure_enabled: {{ cassandra_back_pressure_enabled }}
cassandra_back_pressure_strategy:
	- class_name: org.apache.cassandra.net.RateBasedBackPressure
		parameters:
			- high_ratio: 0.90
				factor: 5
				flow: FAST
